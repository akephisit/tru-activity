# Frontend Development Environment Variables
# Copy this file to .env and modify as needed
# Note: Only variables prefixed with PUBLIC_ can be used in client-side code

# Application Environment
NODE_ENV=development
PUBLIC_ENV=development

# Backend API Configuration
PUBLIC_API_URL=http://localhost:8080
PUBLIC_GRAPHQL_URL=http://localhost:8080/query

# Application Configuration
PUBLIC_APP_NAME="TRU Activity"
PUBLIC_APP_VERSION="1.0.0"

# Optional: WebSocket URL (if different from API URL)
# PUBLIC_WS_URL=ws://localhost:8080/ws

# Optional: Override for production
# PUBLIC_API_URL=https://your-backend-url
# PUBLIC_GRAPHQL_URL=https://your-backend-url/query